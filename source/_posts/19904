oracle ID自增长 使用序列
create sequence autoID increment by 1 start with 1 maxvalue 999999 cycle; 

create table t_sz_tkyth (
  tid number primary key,
  pdate varchar2(20),
  area varchar2(20),
  qwdqfgl number(15,4),
  qwmbfgl number(15,4),
  zxdqfgl number(15,4),
  zxmbfgl number(15,4),
  work_type varchar2(30)
);


